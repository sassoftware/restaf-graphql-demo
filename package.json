  "license": "SEE LICENSE IN license.txt",
  "dependencies": {
    "restaf-graphql-server": "^0.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.15.3"
  }
}
