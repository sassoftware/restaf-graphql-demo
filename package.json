{
  "name": "restaf-graphql-demo",
  "version": "0.2.0",
  "description": "Useful demos of using graphql with SAS Viya",
  "main": "./src/app.js",
  "scripts": {
    "start": "node app.js --env=app.env --appenv=appEnv.js",
    "test": "node app.js --env=../graphqlapp.env --appenv=appEnv.js",
    "test2": "node app.js --env=../graphqlapp2.env --appenv=appEnv.js",
    "debug": "node --inspect-brk app.js --env=../graphqlapp2.env --appenv=appEnv.js",
    "indocker": "node app.js --appenv=testEnv.js",
    "dockerbuild": "docker build -t graphqlapp .",
    "dockerrun": "docker run -p 5000:5000 graphqlapp"
  },
  "repository": "https://github.com/sassoftware/restaf-graphql-demo",
  "keywords": [],
